{
  "name": "bariendo-backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs -d src/data-source/postgres.ts",
    "schema:drop": "yarn typeorm schema:drop",
    "schema:sync": "yarn typeorm schema:sync",
    "migrate": "yarn typeorm migration:run",
    "revert": "yarn typeorm migration:revert",
    "generate": "yarn typeorm migration:generate",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d src/data-source/postgres.ts"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.9",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.5.0",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "{**/*,*}.{js,ts,jsx,tsx,css,md}": "prettier --write",
    ".js,.ts .": " eslint --fix --ext",
    "*.js": "eslint --cache --fix"
  }
}
